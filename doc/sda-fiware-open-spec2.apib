            "numRtwFemales": 40,
            "numReplyFemales": 4,
            "numTWPages": 20,
            "numRtwPages": 10,
            "numReplyPages": 24,
            "numTWUnknown": 20,
            "numRtwUnknown": 10,
            "numReplyUnknown": 24
         }


